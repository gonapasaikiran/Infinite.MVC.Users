        <form asp-controller="AdmissionForm" asp-action="Details" method="post">
        <button type="submit" class="btn btn-danger">Details</button>
        </form>
        </div>*@
        <a asp-controller="AdmissionForm" asp-action="Index">Back to List</a>
    </div>
</div>



